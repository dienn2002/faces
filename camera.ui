  	  border-radius: 8px; 
}

/* --- Title QLabel --- */
QLabel#lblKiemTra {
    font-size: 18px;
    font-weight: bold;
    color: white;
    qproperty-alignment: 'AlignHCenter';
}


/* --- Form Labels --- */
QLabel#lblName, QLabel#lblPlate, #lblCount, #lblRecently, #lblTime{
    color: #d1d5db; /* slate-300 */
    font-weight: bold;
    font-size: 13px;
}

/* --- QLineEdit --- */
QLineEdit#lineEditName, QLineEdit#lineEditPlate, #lineEditCount, #lineEditRecently, #time, #lineEditCheck, #lineEditPlate_2{
    background-color: #374151; /* slate-700 */
