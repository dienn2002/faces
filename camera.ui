    padding: 6px 10px;
    border: 1px solid #4b5563; /* slate-600 */
    border-radius: 6px;
	font-size: 13px;
	font-weight: bold;
}

QLineEdit#lineEditName:focus, QLineEdit#lineEditPlate:focus {
    border: 1px solid #3b82f6; /* blue-500 */
    outline: none;
}

/* --- QPushButton cơ bản --- */
QPushButton#btnVao,
QPushButton#btnRa 
{
    background-color: red; /* emerald-600 */
    color: white;
    font-weight: bold;
    padding: 10px;
    border-radius: 8px;
margin : 10px;
}

QPushButton#btnKiemTra {
    background-color: green; /* emerald-600 */
    color: white;
    font-weight: bold;
    padding: 10px;
    border-radius: 5px;
}

/* --- Hover --- */
QPushButton#btnVao:hover,
QPushButton#btnRa:hover {
    background-color: #059669; /* emerald-700 */
}

QLabel#lblKiemTra{
	 color: #d1d5db; /* slate-300 */
    font-weight: bold;
    font-size: 13px;
}

/* --- Disabled --- */
QPushButton#btnVao:disabled,
QPushButton#btnRa:disabled {
    background-color: #6b7280; /* slate-600 */
    color: #d1d5db; /* slate-300 */
    border: none;
}




#lblKetQua{
	font-size: 20px;
	color: white;
	font-weight: bold;
}</string>
